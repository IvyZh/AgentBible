<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.com.gxdgroup.angentbible">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="cn.com.gxdgroup.angentbible.base.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="cn.com.gxdgroup.angentbible.activities.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.SecondHandHouseActivity" />
        <activity android:name=".activities.SeconHandHouseDetailsActivity" />
        <activity android:name=".activities.VillageInfoActivity" />
    </application>

</manifest>